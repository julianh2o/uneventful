{
  "name": "uneventful",
  "version": "1.0.8",
  "description": "uneventful - Your event management solution",
  "author": {
    "name": "Julian Hartline",
    "url": "https://uneventful.bawdyshop.space"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julianh2o/uneventful"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "twilio": "^5.11.1",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2"
  },
  "scripts": {
    "dev": "concurrently -n client,server -c blue,green \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "PORT=2998 react-scripts start",
    "dev:server": "NODE_ENV=development nodemon --watch server --ext ts --exec 'tsx server/index.ts'",
    "build": "react-scripts build",
    "build:server": "tsc --project server/tsconfig.json",
    "build:local": "bash scripts/build-local.sh",
    "start": "NODE_ENV=production node server/dist/index.js",
    "preview": "yarn build && yarn build:server && yarn start",
    "test": "yarn test:client && yarn test:server",
    "test:client": "vitest run --config vitest.config.ts",
    "test:server": "vitest run --config server/vitest.config.ts",
    "test:watch:client": "vitest watch --config vitest.config.ts",
    "test:watch:server": "vitest watch --config server/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts && vitest run --coverage --config server/vitest.config.ts",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"server/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"server/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"server/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\" \"server/**/*.ts\"",
    "typecheck": "tsc --noEmit && tsc --noEmit --project server/tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "release": "node scripts/release.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/material-ui": "^0.21.18",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "hamburger-react": "^2.5.2",
    "jsdom": "^27.3.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "react-scripts": "^5.0.1",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0",
    "web-vitals": "^5.1.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
